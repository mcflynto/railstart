= form_with url: user_sessions_path, method: :post, local: true do |f|
  .field
    = f.label :email
    %br/
    = f.text_field :email
  .field
    = f.label :password
    %br/
    = f.password_field :password
  .actions
    = f.submit "Login"
  %div
    = f.label "keep me logged in"
    %br/
    = f.check_box :remember
